{
  "env": {
    "es6": true,
    "node": true,
    "jest" : true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "plugins": [
    "jest",
    "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "no-debugger": "warn",
    "no-case-declarations": "off",
    "no-unused-vars": "warn",
    "prettier/prettier": "error"
  }
}
